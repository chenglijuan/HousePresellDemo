<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="zhishusz.housepresell.database.po">
	<class name="Empj_BuildingInfo_AF" table="Empj_BuildingInfo_AF" dynamic-update="true" dynamic-insert="true">
		<id name="tableId">
			<generator class="sequence">
				<param name="sequence">Seq_BldInfoAF</param>
			</generator>
		</id>

		<property name="theState">
			<column name="theState">
				<comment>状态 S_TheState 初始为Normal</comment>
			</column>
		</property>
		<property name="busiState">
			<column name="busiState">
				<comment>业务状态</comment>
			</column>
		</property>
		<property name="eCode">
			<column name="eCode">
				<comment>编号</comment>
			</column>
		</property>
		<many-to-one class="Sm_User" name="userStart" foreign-key="null">
			<column name="userStart">
				<comment>创建人</comment>
			</column>
		</many-to-one>
		<property name="createTimeStamp">
			<column name="createTimeStamp">
				<comment>创建时间</comment>
			</column>
		</property>
		<many-to-one class="Sm_User" name="userUpdate" foreign-key="null">
			<column name="userUpdate">
				<comment>修改人</comment>
			</column>
		</many-to-one>
		<property name="lastUpdateTimeStamp">
			<column name="lastUpdateTimeStamp">
				<comment>最后修改日期</comment>
			</column>
		</property>
		<many-to-one class="Sm_User" name="userRecord" foreign-key="null">
			<column name="userRecord">
				<comment>备案人</comment>
			</column>
		</many-to-one>
		<property name="recordTimeStamp">
			<column name="recordTimeStamp">
				<comment>备案日期</comment>
			</column>
		</property>
		<many-to-one class="Emmp_CompanyInfo" name="developCompany" foreign-key="null">
			<column name="developCompany">
				<comment>关联开发企业</comment>
			</column>
		</many-to-one>
		<property name="eCodeOfDevelopCompany">
			<column name="eCodeOfDevelopCompany">
				<comment>开发企业编号</comment>
			</column>
		</property>
		<many-to-one class="Empj_ProjectInfo" name="project" foreign-key="null">
			<column name="project">
				<comment>关联项目</comment>
			</column>
		</many-to-one>
		<property name="theNameOfProject">
			<column name="theNameOfProject">
				<comment>项目名称-冗余</comment>
			</column>
		</property>
		<property name="eCodeOfProject">
			<column name="eCodeOfProject">
				<comment>项目编号</comment>
			</column>
		</property>
		<many-to-one class="Empj_BuildingInfo" name="building" foreign-key="null">
			<column name="building">
				<comment>关联楼幢</comment>
			</column>
		</many-to-one>
		<property name="eCodeOfBuilding">
			<column name="eCodeOfBuilding">
				<comment>楼幢编号</comment>
			</column>
		</property>
		<property name="buildingArea">
			<column name="buildingArea">
				<comment>建筑面积（㎡）</comment>
			</column>
		</property>
		<property name="escrowArea">
			<column name="escrowArea">
				<comment>托管面积</comment>
			</column>
		</property>
		<property name="deliveryType">
			<column name="deliveryType">
				<comment>交付类型</comment>
			</column>
		</property>
		<property name="upfloorNumber">
			<column name="upfloorNumber">
				<comment>地上楼层数</comment>
			</column>
		</property>
		<property name="downfloorNumber">
			<column name="downfloorNumber">
				<comment>地下楼层数</comment>
			</column>
		</property>
		<property name="landMortgageState">
			<column name="landMortgageState">
				<comment>土地抵押状态</comment>
			</column>
		</property>
		<property name="landMortgagor">
			<column name="landMortgagor">
				<comment>土地抵押权人</comment>
			</column>
		</property>
		<property name="landMortgageAmount">
			<column name="landMortgageAmount">
				<comment>土地抵押金额</comment>
			</column>
		</property>
		<property name="remark">
			<column name="remark" length="4000">
				<comment>备注</comment>
			</column>
		</property>

	</class>
</hibernate-mapping>