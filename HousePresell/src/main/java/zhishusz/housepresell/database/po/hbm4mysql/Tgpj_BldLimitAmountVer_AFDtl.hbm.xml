<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="zhishusz.housepresell.database.po">
	<class name="Tgpj_BldLimitAmountVer_AFDtl" table="Tgpj_BldLimitAmountVer_AFDtl" dynamic-update="true" dynamic-insert="true">
		<id name="tableId">
			<generator class="native"></generator>
		</id>

		<property name="theState">
			<column name="theState">
				<comment>状态 S_TheState 初始为Normal</comment>
			</column>
		</property>
				<property name="busiState">
			<column name="busiState">
				<comment>业务状态</comment>
			</column>
		</property>
		<property name="eCode">
			<column name="eCode">
				<comment>编号</comment>
			</column>
		</property>
		<many-to-one class="Sm_User" name="userStart" foreign-key="null">
			<column name="userStart">
				<comment>创建人</comment>
			</column>
		</many-to-one>
		<property name="createTimeStamp">
			<column name="createTimeStamp">
				<comment>创建时间</comment>
			</column>
		</property>
		<many-to-one class="Sm_User" name="userUpdate" foreign-key="null">
			<column name="userUpdate">
				<comment>最后修改人</comment>
			</column>
		</many-to-one>
		<property name="lastUpdateTimeStamp">
			<column name="lastUpdateTimeStamp">
				<comment>最后修改日期</comment>
			</column>
		</property>
		<many-to-one class="Sm_User" name="userRecord" foreign-key="null">
			<column name="userRecord">
				<comment>备案人</comment>
			</column>
		</many-to-one>
		<property name="recordTimeStamp">
			<column name="recordTimeStamp">
				<comment>备案日期</comment>
			</column>
		</property>
		<many-to-one class="Tgpj_BldLimitAmountVer_AF" name="bldLimitAmountVerMng" foreign-key="null">
			<column name="bldLimitAmountVerMng">
				<comment>关联受限额度主表</comment>
			</column>
		</many-to-one>
		<property name="stageName">
			<column name="stageName">
				<comment>阶段名称</comment>
			</column>
		</property>
		<property name="limitedAmount">
			<column name="limitedAmount">
				<comment>受限额度</comment>
			</column>
		</property>
		<property name="remark">
			<column name="remark" length="4000">
				<comment>备注</comment>
			</column>
		</property>

	</class>
</hibernate-mapping>