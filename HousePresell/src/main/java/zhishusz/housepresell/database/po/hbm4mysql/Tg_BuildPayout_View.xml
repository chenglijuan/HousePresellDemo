<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="zhishusz.housepresell.database.po">
	<class name="Tg_BuildPayout_View" table="Tg_BuildPayout_View" dynamic-update="true" dynamic-insert="true">
		<id name="tableId">
			<generator class="sequence">
				<param name="sequence">Seq_Tg_BuildPayout_View_View</param>
			</generator>
		</id>
		<property name="companyName">
			<column name="companyName">
				<comment>开发企业</comment>
			</column>
		</property>
		<property name="projectName">
			<column name="projectName">
				<comment>项目</comment>
			</column>
		</property>
		<property name="eCodeFroMconstruction">
			<column name="eCodeFroMconstruction">
				<comment>楼幢</comment>
			</column>
		</property>
		<property name="eCodeFromPayoutBill">
			<column name="eCodeFromPayoutBill">
				<comment>资金拨付单号</comment>
			</column>
		</property>
		<property name="currentApplyPayoutAmount">
			<column name="currentApplyPayoutAmount">
				<comment>本次申请支付金额</comment>
			</column>
		</property>
		<property name="currentPayoutAmount">
			<column name="currentPayoutAmount">
				<comment>本次实际支付金额</comment>
			</column>
		</property>
		<property name="payoutDate">
			<column name="payoutDate">
				<comment>拨付日期</comment>
			</column>
		</property>
		<property name="payoutBank">
			<column name="payoutBank">
				<comment>拨付银行</comment>
			</column>
		</property>
		<property name="payoutBankAccount">
			<column name="payoutBankAccount">
				<comment>拨付账号</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>