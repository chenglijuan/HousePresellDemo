<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="zhishusz.housepresell.database.po">
	<class name="Tg_Build_View" table="Tg_Build_View" dynamic-update="true" dynamic-insert="true">
		<id name="tableId">
			<generator class="sequence">
				<param name="sequence">Seq_Tg_Build_View_View</param>
			</generator>
		</id>

		<property name="billTimeStamp">
			<column name="billTimeStamp">
				<comment>记账日期</comment>
			</column>
		</property>
		<property name="companyName">
			<column name="companyName">
				<comment>开发企业</comment>
			</column>
		</property>
		<property name="projectName">
			<column name="projectName">
				<comment>项目</comment>
			</column>
		</property>
		<property name="eCodeFroMconstruction">
			<column name="eCodeFroMconstruction">
				<comment>楼幢</comment>
			</column>
		</property>
		<property name="unitCode">
			<column name="unitCode">
				<comment>单元</comment>
			</column>
		</property>
		<property name="roomId">
			<column name="roomId">
				<comment>房间号</comment>
			</column>
		</property>
		<property name="forEcastArea">
			<column name="forEcastArea">
				<comment>户建筑面积</comment>
			</column>
		</property>
		<property name="contractStatus">
			<column name="contractStatus">
				<comment>合同状态</comment>
			</column>
		</property>
		<property name="contractNo">
			<column name="contractNo">
				<comment>合同编号</comment>
			</column>
		</property>
		<property name="eCodeOfTripleagreement">
			<column name="eCodeOfTripleagreement">
				<comment>三方协议号</comment>
			</column>
		</property>
		<property name="buyerName">
			<column name="buyerName">
				<comment>承购人姓名</comment>
			</column>
		</property>
		<property name="eCodeOfCertificate">
			<column name="eCodeOfCertificate">
				<comment>证件号</comment>
			</column>
		</property>
		<property name="theNameOfCreditor">
			<column name="theNameOfCreditor">
				<comment>借款人姓名</comment>
			</column>
		</property>
		<property name="contractSumPrice">
			<column name="contractSumPrice">
				<comment>合同总价</comment>
			</column>
		</property>
		<property name="paymentMethod">
			<column name="paymentMethod">
				<comment>付款方式</comment>
			</column>
		</property>
		<property name="firstPaymentAmount">
			<column name="firstPaymentAmount">
				<comment>首付款金额</comment>
			</column>
		</property>
		<property name="loanAmount">
			<column name="loanAmount">
				<comment>合同贷款金额</comment>
			</column>
		</property>
		<property name="escrowState">
			<column name="escrowState">
				<comment>托管状态</comment>
			</column>
		</property>
		<property name="contractSignDate">
			<column name="contractSignDate">
				<comment>合同签订日期</comment>
			</column>
		</property>
		<property name="loanAmountIn">
			<column name="loanAmountIn">
				<comment>贷款入账金额</comment>
			</column>
		</property>
		<property name="theAmountOfRetainedequity">
			<column name="theAmountOfRetainedequity">
				<comment>留存权益</comment>
			</column>
		</property>
		<property name="statementState">
			<column name="statementState">
				<comment>对账状态</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>