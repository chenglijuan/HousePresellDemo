<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="zhishusz.housepresell.database.po">
	<class name="Tg_BankLoanInSituation_View" table="Tg_BankLoanInSituation_View" dynamic-update="true" dynamic-insert="true">
		<id name="tableId">
			<generator class="sequence">
				<param name="sequence">Seq_Tg_BankLoanInSituation_View_View</param>
			</generator>
		</id>
		<property name="companyName">
			<column name="companyName">
				<comment>企业名称</comment>
			</column>
		</property>
		<property name="projectName">
			<column name="projectName">
				<comment>项目</comment>
			</column>
		</property>
		<property name="escrowAcount">
			<column name="escrowAcount">
				<comment>托管账户</comment>
			</column>
		</property>
		<property name="escrowAcountShortName">
			<column name="escrowAcountShortName">
				<comment>托管账户简称</comment>
			</column>
		</property>
		<property name="billTimeStamp">
			<column name="billTimeStamp">
				<comment>入账日期</comment>
			</column>
		</property>
		<property name="loanAmountIn">
			<column name="loanAmountIn">
				<comment>入账金额</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>